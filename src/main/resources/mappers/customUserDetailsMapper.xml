<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.inspirational.userDetails.CustomUserDetailsDAO">
	<select id="selectUserByLoginId"
		resultType="com.inspirational.userDetails.CustomUserDetails">
		select
		*
		from
		`user`
		where
		loginId = #{loginId}
	</select>
</mapper>